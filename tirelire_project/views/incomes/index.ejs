<%- include('../partials/header') %>

<table id="list">
    <thead>
        <tr>
            <th>Name</th>
            <th>Date</th>
            <th>Amount</th>
        </tr>
    </thead>

    <tbody>
        <% incomes.forEach(function(inc) { %>
              <tr>
                  <td><%= inc.name %></td>
                  <td><%= inc.date %></td>
                  <td><%= inc.amount %></td>
                  <td><a href="/incomes/<%= inc._id %>">DETAILS</a></td>
              </tr>
          <% }); %>
    </tbody>
</table> 
<%- include('../partials/footer') %>