<%- include('../partials/header') %>
<h4>Incomes</h4>
<table id="list">
    <thead>
        <tr>
            <th>Name</th>
            <th>Date</th>
            <th>Amount</th>
        </tr>
    </thead>

    <tbody>
        <% incomes.forEach(function(inc) { %>
              <tr>
                  <td>  <%=  inc.name %></td>
                  <td>  <%=  inc.date %></td>
                  <td> $ <%=  inc.amount %></td>
                  <td><a href="/incomes/<%= inc._id %>">DETAILS</a></td>
              </tr>
          <% }); %>
    </tbody>
</table> 
<br>
<br>
<h4>Expenses</h4>
<table id="list">
    <thead>
        <tr>
            <th>Store</th>
            <th>Date</th>
            <th>Amount</th>
        </tr>
    </thead>

    <tbody>
        <% expenses.forEach(function(exp) { %>
              <tr>
                  <td>  <%= exp.store %></td>
                  <td>  <%= exp.date %></td>
                  <td> $ <%= exp.amount %></td>
                  <td><a href="/expenses/<%= exp._id %>">DETAILS</a></td>
              </tr>
          <% }); %>
    </tbody>
</table>
<%- include('../partials/footer') %>